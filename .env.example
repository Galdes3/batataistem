# Server
PORT=3000
NODE_ENV=development

# Instagram Graph API
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token_here
INSTAGRAM_APP_ID=your_instagram_app_id_here
INSTAGRAM_APP_SECRET=your_instagram_app_secret_here

# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# Database (Supabase PostgreSQL)
# Formato: postgresql://postgres:[SENHA]@db.[PROJETO-ID].supabase.co:5432/postgres
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[PROJETO-ID].supabase.co:5432/postgres

# Cron Job (em formato cron: minuto hora dia mÃªs dia-da-semana)
# Exemplo: "0 * * * *" = a cada hora
CRON_SCHEDULE="0 * * * *"